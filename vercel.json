{
  "builds": [
    { "src": "web/**", "use": "@vercel/static" },
    { "src": "demo/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/web/index.html" },
    { "src": "^/ui$", "dest": "/web/index.html" },
    { "src": "^/ui/demo/(.*)$", "dest": "/demo/$1" },
    { "src": "^/ui/(.*)$", "dest": "/web/$1" }
  ],
  "headers": [
    {
      "source": "/ui/(.*).json",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
