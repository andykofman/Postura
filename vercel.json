{
  "rewrites": [
    { "source": "/", "destination": "/web/index.html" },
    { "source": "/ui", "destination": "/web/index.html" },
    { "source": "/ui/demo/:path*", "destination": "/demo/:path*" },
    { "source": "/ui/:path*", "destination": "/web/:path*" }
  ],
  "headers": [
    {
      "source": "/ui/(.*).json",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
